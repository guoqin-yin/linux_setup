syntax	on

set noerrorbells
set number
colorscheme molokai
set tabstop=4
set shiftwidth=4
set autoindent
set expandtab
set softtabstop=4
set cursorline
set smartindent
set incsearch
set hlsearch
set smartcase
set clipboard=unnamedplus
map <C-j> <C-W>j
map <C-k> <C-W>k
map <C-h> <C-W>h
map <C-l> <C-W>l
